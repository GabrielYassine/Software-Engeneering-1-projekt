<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dtu.app.ui.pages.MenuController">
    <children>
        <Button fx:id="viewProjectsButton" onAction="#switchToProjects" text="View Projects"/>
        <Button fx:id="viewEmployeesButton" onAction="#switchToEmployees" text="View Employees"/>
        <Button fx:id="viewEmailButton" onAction="#switchToEmail" text="View Email"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>